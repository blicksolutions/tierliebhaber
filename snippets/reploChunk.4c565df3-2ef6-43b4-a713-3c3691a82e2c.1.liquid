          const newUrl = `${originalUrl}${separator}attributes[__clickid]=${final_cid}`;
          link.setAttribute('href', newUrl);
        }
        else {
          const separator = originalUrl.includes('?') ? '&' : '?';
          const newUrl = `${originalUrl}${separator}rtkcid=${final_cid}&clickid=${final_cid}&cmpid=${cmpid}`;
          link.setAttribute('href', newUrl);
        }
      });

      // For Images 

      const imageLinks = document.querySelectorAll('img[data-imagelink]');
      imageLinks.forEach(img => {
        const originalUrl = img.getAttribute('data-imagelink');

        if (originalUrl && originalUrl.includes('/cart')) {
          const separator = originalUrl.includes('?') ? '&' : '?';
          const newUrl = `${originalUrl}${separator}attributes[__clickid]=${final_cid}`;
          img.setAttribute('data-imagelink', newUrl);
