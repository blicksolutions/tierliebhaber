        else {
          const separator = originalUrl.includes('?') ? '&' : '?';
          const newUrl = `${originalUrl}${separator}rtkcid=${final_cid}&clickid=${final_cid}&cmpid=${cmpid}`;
          img.setAttribute('data-imagelink', newUrl);
        }

      });

    } else {
      console.log("NO CLICKID FOUND: ",)
      // If clickid is not found, check again after 1 second
      setTimeout(appendClickIdToUrl, 1000);
    }
  }

