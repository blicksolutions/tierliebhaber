                  <a href="/" class="Footer__socials__social">
                    <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" viewBox="0 0 28 28" fill="none">
                      <path d="M10.0136 16.0238C10.0135 16.3081 10.0694 16.5897 10.1782 16.8524C10.2869 17.1151 10.4463 17.3538 10.6473 17.5549C10.8483 17.7559 11.087 17.9154 11.3496 18.0243C11.6123 18.1331 11.8938 18.1891 12.1781 18.1891C12.7582 18.1978 13.3186 17.9788 13.739 17.5791C14.1595 17.1793 14.4065 16.6307 14.4271 16.051L14.4519 6.30496H16.4513C16.542 7.17347 16.9353 7.98241 17.5622 8.59031C18.1891 9.19821 19.0097 9.56638 19.8806 9.63041V11.9531C19.7666 11.964 19.6518 11.9702 19.5371 11.9702C18.9184 11.9706 18.3093 11.8168 17.765 11.5227C17.2206 11.2286 16.7582 10.8035 16.4195 10.2857V16.0246C16.4195 16.8635 16.1707 17.6835 15.7047 18.381C15.2386 19.0785 14.5762 19.6221 13.8012 19.9431C13.0262 20.2641 12.1734 20.3481 11.3507 20.1845C10.5279 20.0208 9.77219 19.6169 9.17903 19.0237C8.58587 18.4305 8.18192 17.6748 8.01827 16.8521C7.85461 16.0293 7.93861 15.1765 8.25962 14.4015C8.58064 13.6265 9.12426 12.9641 9.82175 12.4981C10.5192 12.032 11.3393 11.7833 12.1781 11.7833C12.2665 11.7833 12.3534 11.791 12.4402 11.7965V13.8904C12.3535 13.8765 12.2659 13.8677 12.1781 13.864C11.6049 13.864 11.055 14.0914 10.6492 14.4963C10.2434 14.9012 10.0149 15.4506 10.0136 16.0238Z" fill="#525252"/>
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M2.1512 4.5892C1.41492 6.03424 1.41492 7.92591 1.41492 11.7092V14.8617C1.41492 18.645 1.41492 20.5367 2.1512 21.9817C2.79886 23.2528 3.83229 24.2863 5.10339 24.9339C6.54843 25.6702 8.4401 25.6702 12.2234 25.6702H15.3759C19.1592 25.6702 21.0509 25.6702 22.4959 24.9339C23.767 24.2863 24.8005 23.2528 25.4481 21.9817C26.1844 20.5367 26.1844 18.645 26.1844 14.8617V11.7092C26.1844 7.92591 26.1844 6.03424 25.4481 4.5892C24.8005 3.31809 23.767 2.28467 22.4959 1.63701C21.0509 0.900726 19.1592 0.900726 15.3759 0.900726H12.2234C8.4401 0.900726 6.54843 0.900726 5.10339 1.63701C3.83229 2.28467 2.79886 3.31809 2.1512 4.5892ZM15.3759 3.1525H12.2234C10.2946 3.1525 8.98341 3.15426 7.96992 3.23705C6.9827 3.31771 6.47785 3.46391 6.12567 3.64335C5.27827 4.07513 4.58932 4.76408 4.15754 5.61148C3.9781 5.96366 3.8319 6.4685 3.75124 7.45573C3.66845 8.46922 3.66669 9.78041 3.66669 11.7092V14.8617C3.66669 16.7906 3.66845 18.1017 3.75124 19.1152C3.8319 20.1025 3.9781 20.6073 4.15754 20.9595C4.58932 21.8069 5.27827 22.4958 6.12567 22.9276C6.47785 23.107 6.9827 23.2533 7.96992 23.3339C8.98341 23.4167 10.2946 23.4185 12.2234 23.4185H15.3759C17.3048 23.4185 18.6159 23.4167 19.6294 23.3339C20.6167 23.2533 21.1215 23.107 21.4737 22.9276C22.3211 22.4958 23.01 21.8069 23.4418 20.9595C23.6212 20.6073 23.7675 20.1025 23.8481 19.1152C23.9308 18.1017 23.9326 16.7906 23.9326 14.8617V11.7092C23.9326 9.78041 23.9308 8.46922 23.8481 7.45573C23.7675 6.4685 23.6212 5.96366 23.4418 5.61148C23.01 4.76408 22.3211 4.07513 21.4737 3.64335C21.1215 3.46391 20.6167 3.31771 19.6294 3.23705C18.6159 3.15426 17.3048 3.1525 15.3759 3.1525Z" fill="#525252"/>
