      <script id="custom-js-gSCC3bCBrQ">
        try {
          /* TLH Flavor Slider – MULTI INSTANCE SAFE (free scroll, endless) */
(function(){
  function enhanceSection(sec){
    if (!sec || sec.dataset.tlhInit === '1') return;
    sec.dataset.tlhInit = '1';

    const track   = sec.querySelector('.tlh-flavor-track');
    const btnPrev = sec.querySelector('.tlh-nav.prev');
    const btnNext = sec.querySelector('.tlh-nav.next');
    const countEl = sec.querySelector('.tlh-count-num');
    const peek    = sec.querySelector('.tlh-peek-hint');

    if (!track || track.dataset.tlhBuilt === '1') return;

    // --- Originals (vor dem Klonen) ---
    const originals = Array.from(track.children);
    const N = originals.length;

    // Absicherung: nicht doppelt initialisieren
    if (N === 0) return;
    track.dataset.tlhBuilt = '1';

    // --- Triple-Belt für Endlos-Scroll ---
    const prefix = originals.map(n => n.cloneNode(true));
    const suffix = originals.map(n => n.cloneNode(true));
    prefix.forEach(n => track.insertBefore(n, track.firstChild));
    suffix.forEach(n => track.appendChild(n));

    const all = Array.from(track.children); // 3N
    let step = 0;

    function measureStep(){
      const a = all[N], b = all[N+1];
      step = Math.max(1, Math.round(b.offsetLeft - a.offsetLeft));
    }
    function setScrollX(px){ track.scrollLeft = px; }

    function logicalIndexFromX(x){
      const approx = Math.round(x / step) - N;      // 0..N-1 ungefähr
      return ((approx % N) + N) % N;
    }
    function updateCounterFromX(x){
      if (countEl) countEl.textContent = logicalIndexFromX(x) + 1;
    }

    function normalizeIfNeeded(){
      const x = track.scrollLeft;
      const leftEdge  = (N - 4) * step;             // großzügige Puffer
      const rightEdge = (2*N + 4) * step;
      if (x < leftEdge)  setScrollX(x + N*step);
      if (x > rightEdge) setScrollX(x - N*step);
    }

    function ready(){
      measureStep();
      setScrollX(N * step);                         // mittleres Band
      updateCounterFromX(track.scrollLeft);
      // kleiner Peek-Hinweis mobil
      setTimeout(()=>{
        if (peek && window.innerWidth < 860){
          peek.style.display='inline-flex';
          setTimeout(()=>peek.style.display='none', 1600);
        }
      }, 400);
    }

    if (document.readyState === 'complete') ready();
    else window.addEventListener('load', ready);

    function go(dir){
      normalizeIfNeeded();
      const currentDomIndex = Math.round(track.scrollLeft / step);
      const target = currentDomIndex + dir;
      track.scrollTo({ left: target * step, behavior: 'smooth' });
      updateCounterFromX(target * step);
    }
    btnNext && btnNext.addEventListener('click', ()=>go(+1));
    btnPrev && btnPrev.addEventListener('click', ()=>go(-1));

    // Live-Update + Endlos-Normalisierung
    track.addEventListener('scroll', ()=>{
      normalizeIfNeeded();
      updateCounterFromX(track.scrollLeft);
    }, {passive:true});

    // Desktop Drag (pro Instanz)
    let dragging=false, sx=0, sl=0;
    track.addEventListener('pointerdown', e=>{
      dragging=true; track.setPointerCapture(e.pointerId);
      sx=e.clientX; sl=track.scrollLeft;
    });
    track.addEventListener('pointermove', e=>{
      if(!dragging) return;
      setScrollX(sl - (e.clientX - sx));
    });
    track.addEventListener('pointerup', ()=>{ dragging=false; });

    // Resize nur für diese Sektion
    const onResize = ()=>{
      const li = logicalIndexFromX(track.scrollLeft);
      measureStep();
      setScrollX((N + li) * step);
      updateCounterFromX(track.scrollLeft);
    };
    window.addEventListener('resize', onResize);
  }

  // Alle vorhandenen Sektionen initialisieren
  document.querySelectorAll('.tlh-flavor-section').forEach(enhanceSection);

  // Optional: falls Builder nachträglich Sektionen einfügt
  const mo = new MutationObserver(() => {
    document.querySelectorAll('.tlh-flavor-section:not([data-tlh-init])')
      .forEach(enhanceSection);
  });
  mo.observe(document.documentElement, { childList:true, subtree:true });
})();

        } catch(err){}
