<script>
document.addEventListener("DOMContentLoaded", () => {
  const cards = document.querySelectorAll(".tl-offer-card");
  const gifts = document.querySelector(".tl-freegifts");

  function setGifts(isBundle){
    if(!gifts) return;
    if(isBundle){
      gifts.classList.add("tl-freegifts--active");
      gifts.classList.remove("tl-freegifts--inactive");
    }else{
      gifts.classList.remove("tl-freegifts--active");
      gifts.classList.add("tl-freegifts--inactive");
    }
  }

  // Emoji-Burst leicht √ºber der Geschenkbox (unten im Card-Bereich)
  function triggerEmojiBurst(card){
    const num = 7;
    const cardRect = card.getBoundingClientRect();
    const cardHeight = cardRect.height;

    for(let i=0;i<num;i++){
      const span = document.createElement("span");
      span.className = "tl-emoji-burst";
      span.textContent = Math.random()>0.5 ? "üéÅ" : "üéâ";

      // weiter unten im Card-Bereich (70‚Äì88%)
      const left = 30 + Math.random()*40;   // 30‚Äì70%
      const top  = 70 + Math.random()*18;   // 70‚Äì88%

      span.style.left = left + "%";
      span.style.top  = top + "%";

      card.appendChild(span);
      setTimeout(()=>span.remove(),1150);
    }
  }

  // initial: Bundle aktiv
  setGifts(true);

  cards.forEach(card => {
    card.addEventListener("click", e => {
      if(e.target.tagName.toLowerCase()==="a") return; // CTA nicht abfangen

      cards.forEach(c => c.classList.remove("is-active"));
      card.classList.add("is-active");

      const isBundle = card.dataset.offer === "bundle";
      setGifts(isBundle);

      if(isBundle){
        triggerEmojiBurst(card);
      }
    });
  });
});
</script>
