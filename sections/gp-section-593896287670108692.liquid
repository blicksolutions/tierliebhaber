        giftsBox.classList.remove("tl-freegifts--inactive");
      } else {
        giftsBox.classList.add("tl-freegifts--inactive");
        giftsBox.classList.remove("tl-freegifts--active");
      }
    }

    // initial: Bundle aktiv
    setGiftState(true);

    cards.forEach(card => {
      card.addEventListener("click", function(e) {
        // wenn direkt auf CTA geklickt wird, Darstellung nicht beeinflussen
        if (e.target.tagName.toLowerCase() === "a") return;

        cards.forEach(c => c.classList.remove("is-active"));
        card.classList.add("is-active");

        if (card.dataset.offer === "bundle") {
          setGiftState(true);
        } else {
          setGiftState(false);
        }
      });
    });
  });
</script>
