<script>
  document.addEventListener("DOMContentLoaded", function() {
    const cards = document.querySelectorAll(".tl-offer-card");
    const giftsBox = document.querySelector(".tl-freegifts");

    cards.forEach(card => {
      card.addEventListener("click", function(e) {
        // CTA-Klick nicht doppelt √ºberschreiben
        if (e.target.tagName.toLowerCase() === "a") return;

        cards.forEach(c => c.classList.remove("is-active"));
        card.classList.add("is-active");

        if (card.dataset.offer === "bundle") {
          giftsBox.classList.add("tl-freegifts-visible");
        } else {
          giftsBox.classList.remove("tl-freegifts-visible");
        }
      });
    });
  });
